            <copy bpelx:insertMissingToData="yes">
              <from>ora:getFaultName()</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error/cmn:ShortDescription</to>
            </copy>
            <copy bpelx:insertMissingToData="yes">
              <from>concat('There have been exceptions when trying set values in variables. ',ora:getFaultAsString())</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error/cmn:Description</to>
            </copy>
            <copy bpelx:insertMissingToData="yes">
              <from><literal>[UpdateSites]</literal></from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error/cmn:BusinessProcess</to>
            </copy>
            <copy bpelx:insertMissingToData="yes">
              <from><literal>[CustomerBiz]</literal></from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error/cmn:FailedService</to>
            </copy>
            <copy bpelx:insertMissingToData="yes">
              <from>xp20:current-dateTime()</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error/cmn:TimeStamp</to>
            </copy>
            <copy bpelx:insertMissingToData="yes">
              <from><literal><Element xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal></from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Success</to>
            </copy>
            <extensionAssignOperation>
              <bpelx:remove>
                <?audit suppress oracle.ide.xml.validation-incomplete?>
                <bpelx:target expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Success</bpelx:target>
              </bpelx:remove>
            </extensionAssignOperation>
            <copy bpelx:insertMissingToData="yes">
              <from><literal><Element xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal></from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return</to>
            </copy>
            <extensionAssignOperation>
              <bpelx:remove>
                <?audit suppress oracle.ide.xml.validation-incomplete?>
                <bpelx:target expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return</bpelx:target>
              </bpelx:remove>
            </extensionAssignOperation>
          </assign>
        </catch>
      </faultHandlers>
      <sequence name="sequenceUpdateSites">
        <documentation>
          <![CDATA[Secuencia principal para la lógica de la operación que representa el BPEL.]]>
        </documentation>
        <assign name="assignRq">
          <copy ignoreMissingFromData="yes">
            <from><literal><Element xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Success</to>
          </copy>
          <copy>
            <from><literal><Element xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors</to>
          </copy>
          <extensionAssignOperation>
            <bpelx:remove>
              <bpelx:target expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors</bpelx:target>
            </bpelx:remove>
          </extensionAssignOperation>
          <copy ignoreMissingFromData="yes">
            <from><literal>PRIMARY</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerPrimarySiteRq.GetCustomerSitesRq/ns1:PSiteSrc</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from>$inputVariable.UpdateSitesRq/ns0:CustomerId</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerPrimarySiteRq.GetCustomerSitesRq/ns1:PCustomerId</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>BILL</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerBillSitesRq.GetCustomerSitesRq/ns1:PSiteSrc</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from>$inputVariable.UpdateSitesRq/ns0:CustomerId</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerBillSitesRq.GetCustomerSitesRq/ns1:PCustomerId</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>BILL</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerShipSitesRq.GetCustomerSitesRq/ns1:PSiteSrc</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from>$inputVariable.UpdateSitesRq/ns0:CustomerId</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerShipSitesRq.GetCustomerSitesRq/ns1:PCustomerId</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>UpdateSites</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:OperationName</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>Success</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Estaus</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>0</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:SuccessRecordCount</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>0</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:ErrorRecordCount</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from><literal>0</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:TotalRecordCount</to>
          </copy>
        </assign>
        <invoke name="invokeWsFinancialsTecGetCustomerSitesPrimary" bpelx:invokeAsDetail="no"
                partnerLink="wsFinancialsTec" portType="ns1:FinancialsTecPortType" operation="GetCustomerSites"
                inputVariable="varGetCustomerPrimarySiteRq" outputVariable="varGetCustomerPrimarySiteRs"/>
        <invoke name="invokeWsFinancialsTecGetCustomerSitesBillTo" bpelx:invokeAsDetail="no"
                partnerLink="wsFinancialsTec" portType="ns1:FinancialsTecPortType" operation="GetCustomerSites"
                inputVariable="varGetCustomerBillSitesRq" outputVariable="varGetCustomerBillSitesRs"/>
        <invoke name="invokeWsFinancialsTecGetCustomerSitesShipTo" bpelx:invokeAsDetail="no"
                partnerLink="wsFinancialsTec" portType="ns1:FinancialsTecPortType" operation="GetCustomerSites"
                inputVariable="varGetCustomerShipSitesRq" outputVariable="varGetCustomerShipSitesRs"/>
        <assign name="trMakeSiteCollection">
          <bpelx:annotation>
            <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
          </bpelx:annotation>
          <copy ignoreMissingFromData="yes">
            <from>ora:doXSLTransformForDoc("../Transformations/XSLs/trMakeSiteCollection.xsl", $varGetCustomerPrimarySiteRs.GetCustomerSitesRs, "varGetCustomerBillSitesRs.GetCustomerSitesRs", $varGetCustomerBillSitesRs.GetCustomerSitesRs, "varGetCustomerShipSitesRs.GetCustomerSitesRs", $varGetCustomerShipSitesRs.GetCustomerSitesRs)</from>
            <to variable="varAllSites"/>
          </copy>
        </assign>
        <forEach parallel="no" counterName="varCurrSiteIdx" name="forEachSite">
          <startCounterValue>1</startCounterValue>
          <finalCounterValue>count($varAllSites/ns1:Return/ns1:CustomerSite)</finalCounterValue>
          <scope name="Scope1">
            <variables>
              <variable name="varCurrSite" type="ns1:CustomerSiteType"/>
              <variable name="varGetCustomerSiteLogRq" messageType="ns3:GetCustomerSiteLogRq"/>
              <variable name="varGetCustomerSiteLogRs" messageType="ns3:GetCustomerSiteLogRs"/>
              <variable name="varMergeLocationRq" messageType="ns2:MergeLocationRq"/>
              <variable name="varMergeLocationRs" messageType="ns2:MergeLocationRs"/>
              <variable name="varUpdateCustomerSitesRq" messageType="ns1:UpdateCustomerSitesRq"/>
              <variable name="varUpdateCustomerSitesRs" messageType="ns1:UpdateCustomerSitesRs"/>
              <variable name="varGetCountryCodeRq" type="ns0:GetCountryCodeRqType"/>
              <variable name="varGetCountryCodeRs" type="ns0:GetCountryCodeRsType"/>
              <variable name="varCreateSiteRq" messageType="ns4:CreateSiteRq"/>
              <variable name="varCreateSiteRs" messageType="ns4:CreateSiteRs"/>
              <variable name="varErrorRecordCount" type="xsd:integer"/>
            </variables>
            <sequence name="Sequence1">
              <assign name="assignCurrentSite">
                <copy ignoreMissingFromData="yes">
                  <from>$varAllSites/ns1:Return/ns1:CustomerSite[$varCurrSiteIdx]</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCurrSite</to>
                </copy>
                <copy ignoreMissingFromData="yes">
                  <from>number($outputVariable.UpdateSitesRs/ns0:Return/ns0:TotalRecordCount) +1</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:TotalRecordCount</to>
                </copy>
              </assign>
              <assign name="assignFindLocationId">
                <copy ignoreMissingFromData="yes">
                  <from>$varCurrSite/ns1:CustomerSiteId</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerSiteLogRq.GetCustomerSiteLogRq/ns3:CustomerSite/ns3:CustomerSiteId</to>
                </copy>
                <copy ignoreMissingFromData="yes">
                  <from>$varCurrSite/ns1:SiteUse</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCustomerSiteLogRq.GetCustomerSiteLogRq/ns3:CustomerSite/ns3:UseCode</to>
                </copy>
              </assign>
              <invoke name="invokeWsAuditControlTecFindLocationId" bpelx:invokeAsDetail="no"
                      partnerLink="wsAuditControlTec" portType="ns3:AuditControlTecPortType"
                      operation="GetCustomerSiteLog" inputVariable="varGetCustomerSiteLogRq"
                      outputVariable="varGetCustomerSiteLogRs"/>
              <if name="IfLocationNotExists">
                <documentation>
                  <![CDATA[Not exists]]>
                </documentation>
                <condition>(string-length($varGetCustomerSiteLogRs.GetCustomerSiteLogRs/ns3:Return/ns3:CustomerSiteLog/ns3:LocationId ) =0 and string-length($varGetCustomerSiteLogRs.GetCustomerSiteLogRs/ns3:Return/ns3:CustomerSiteLog/ns3:PartySiteId ) = 0) or contains($varGetCustomerSiteLogRs.GetCustomerSiteLogRs/ns3:Return/ns3:CustomerSiteLog[1]/ns3:CustAcctSiteId , '0')</condition>
                <sequence name="sequenceCreateSite">
                  <assign name="assignCreateSite">
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$inputVariable.UpdateSitesRq/ns0:PartyId</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:PartyId</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$inputVariable.UpdateSitesRq/ns0:PartyType</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:CustomerPartyType</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$inputVariable.UpdateSitesRq/ns0:CustomerAccountId</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:CustomerAccountId</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:SiteName</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:SiteName</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:SiteDescription</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:SiteDescription</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:CustomerSiteId</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:LegacyCustomerSiteId</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:SiteUse</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:SiteUseCode</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:AccountAddressSet</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:AccountAddressSet</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" keepSrcElementName="no" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:StreetName</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:StreetName</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:ExternalNumber</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:ExternalNumber</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:InternalNumber</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:InternalNumber</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:PostalCode</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:PostalCode</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:Province</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:Province</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:City</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:City</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:State</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:State</to>
                    </copy>
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varCurrSite/ns1:Country</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateSiteRq.CreateSiteRq/ns0:Site/ns0:Country</to>
                    </copy>
                  </assign>
                  <invoke name="invokeBpelCreateSite" bpelx:invokeAsDetail="no"
                          partnerLink="bpelCreateSite.bpelCreateSite" portType="ns4:bpelCreateSite"
                          operation="CreateSite" inputVariable="varCreateSiteRq" outputVariable="varCreateSiteRs"/>
                  <if name="IfSuccess">
                    <documentation>
                      <![CDATA[Success]]>
                    </documentation>
                    <condition>count($varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error) = 0</condition>
                    <assign name="assignSuccessRs">
                      <copy>
                        <from>number($outputVariable.UpdateSitesRs/ns0:Return/ns0:SuccessRecordCount) +1</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:SuccessRecordCount</to>
                      </copy>
                    </assign>
                    <else>
                      <documentation>
                        <![CDATA[Error]]>
                      </documentation>
                      <assign name="assignErrorRs">
                        <copy>
                          <from><literal>Failed</literal></from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Estaus</to>
                        </copy>
                        <copy>
                          <from>number($outputVariable.UpdateSitesRs/ns0:Return/ns0:ErrorRecordCount)+1</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:ErrorRecordCount</to>
                        </copy>
                        <copy>
                          <from>$outputVariable.UpdateSitesRs/ns0:Return/ns0:ErrorRecordCount</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varErrorRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:ErrorCode</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:ErrorCode</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:ShortDescription</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:ShortDescription</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:Description</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:Description</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:BusinessProcess</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:BusinessProcess</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:FailedService</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:FailedService</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]/cmn:TimeStamp</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/cmn:TimeStamp</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>concat('Sitio[',$varCurrSite/ns1:CustomerSiteId,'] del cliente [',$varCurrSite/ns1:CustomerId,']' )</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/ns0:Return/ns0:Errors/ns0:RecordError[$varErrorRecordCount]/ns0:RecordKey</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$varCreateSiteRs.CreateSiteRs/cmn:Errors/cmn:Error[1]</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.UpdateSitesRs/cmn:Errors/cmn:Error</to>
                        </copy>
                      </assign>
                    </else>
                  </if>
                </sequence>
                <else>
                  <documentation>
                    <![CDATA[Exists site]]>
                  </documentation>
                  <sequence name="sequenceUpdateSite">
                    <assign name="assignCountryName">
                      <copy ignoreMissingFromData="yes">
                        <from>$varAllSites/ns1:Return/ns1:CustomerSite[1]/ns1:Country</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetCountryCodeRq/ns0:CountryName</to>
                      </copy>
                    </assign>
                    <extensionActivity>
                      <bpelx:call name="callGetCountryCode"
                                  xmlns:sp1="http://soa.estrellaroja.com.mx/CustomerBiz/sbpelGetCountryCode"
                                  target="sp1:sbpelGetCountryCode">
                        <bpelx:param name="varGetCountryCodeRq" copyByValue="yes" variable="varGetCountryCodeRq"/>
                        <bpelx:param name="varGetCountryCodeRs" copyByValue="no" variable="varGetCountryCodeRs"/>
                      </bpelx:call>
                    </extensionActivity>
                    <if name="IfCountryNotExists">
                      <documentation>
                        <![CDATA[Not Exists]]>
                      </documentation>
                      <condition>count($varGetCountryCodeRs/cmn:Errors/cmn:Error) &gt; 0 and string-length($varGetCountryCodeRs/ns0:Return/ns0:CountryCode) = 0</condition>
                      <empty name="continue"/>
                      <else>
                        <documentation>
                          <![CDATA[Exists]]>
                        </documentation>
                        <sequence name="sequenceCountryCodeExists">
                          <assign name="assignMergeUpdateSiteRq">
                            <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                              <from>$varCurrSite/ns1:PostalCode</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varMergeLocationRq.MergeLocationRq/ns2:Location/ns2:PostalCode</to>
                            </copy>
