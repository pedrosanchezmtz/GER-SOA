          </copy>
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
            <from><literal>0</literal></from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:ErrorRecordCount</to>
