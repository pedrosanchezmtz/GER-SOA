                          <from>$varUpdateCustomerRs.UpdateCustomerRs/ns0:Return</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varOperationResult</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:SuccessRecordCount)+number($varUpdateCustomerRs.UpdateCustomerRs/ns0:Return/ns0:SuccessRecordCount)</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:SuccessRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($varUpdateCustomerRs.UpdateCustomerRs/ns0:Return/ns0:ErrorRecordCount) + number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:ErrorRecordCount)</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:ErrorRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:TotalRecordCount) +1</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:TotalRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:SuccessRecordCount) + number($varUpdateCustomerRs.UpdateCustomerRs/ns0:Return/ns0:NestedOperation/ns0:SuccessRecordCount)</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:SuccessRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:ErrorRecordCount) + number($varUpdateCustomerRs.UpdateCustomerRs/ns0:Return/ns0:NestedOperation/ns0:ErrorRecordCount)</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:ErrorRecordCount</to>
                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:TotalRecordCount) + number($varUpdateCustomerRs.UpdateCustomerRs/ns0:Return/ns0:TotalRecordCount)</from>
                          <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:UpdateResult/ns0:NestedOperation/ns0:TotalRecordCount</to>
                        </copy>
                      </assign>
                    </sequence>
                    <elseif>
                      <documentation>
                        <![CDATA[CreateCustomer]]>
                      </documentation>
                      <condition>$varFindErrorRs/cmn:Error/cmn:ErrorCode = 'SOA-00010'</condition>
                      <sequence name="Sequence2">
                        <assign name="assignCreateCustomerRq">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:CustomerId</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:CustomerId</to>
                          </copy>
                          <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:Name</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:Name</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:LastName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:LastName</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:PaymentTerms</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:PaymentTerms</to>
                          </copy>
                          <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:FiscalCode</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:FiscalCode</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:AccountName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:AccountName</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:CustomerType</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCreateCustomerRq.CreateCustomerRq/ns0:CustomerData/ns0:CustomerType</to>
                          </copy>
                        </assign>
                        <invoke name="invokeCreateCustomer" bpelx:invokeAsDetail="no"
                                partnerLink="bpelCreateCustomer.bpelCreateCustomer" portType="ns2:bpelCreateCustomer"
                                operation="CreateCustomer" inputVariable="varCreateCustomerRq"
                                outputVariable="varCreateCustomerRs"/>
                        <assign name="assignCreateRs">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varCreateCustomerRs.CreateCustomerRs/ns0:Return</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varOperationResult</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>number($varCreateCustomerRs.CreateCustomerRs/ns0:Return/ns0:SuccessRecordCount) + number($outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:SuccessRecordCount)</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:SuccessRecordCount</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>number($varCreateCustomerRs.CreateCustomerRs/ns0:Return/ns0:ErrorRecordCount) + number($outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:ErrorRecordCount)</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:ErrorRecordCount</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:TotalRecordCount) +1</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:TotalRecordCount</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>number($outputVariable.SyncCustomersRs/ns0:Return/ns0:CreateResult/ns0:NestedOperation/ns0:SuccessRecordCount) + number($varCreateCustomerRs.CreateCustomerRs/ns0:Return/ns0:NestedOperation/ns0:SuccessRecordCount)</from>
