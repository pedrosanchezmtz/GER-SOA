        <invoke name="invokeWsFinancialsTecGetCustomers" bpelx:invokeAsDetail="no" partnerLink="wsFinancialsTec"
                portType="ns1:FinancialsTecPortType" operation="GetCustomers" inputVariable="varGetCustomersRq"
                outputVariable="varGetCustomersRs"/>
        <if name="IfHasCustomers">
          <documentation>
            <![CDATA[HasCustomer]]>
          </documentation>
          <condition>count($varGetCustomersRs.GetCustomersRs/ns1:Return/ns1:Customer) &gt;0</condition>
          <sequence name="sequenceSyncCus">
            <forEach parallel="no" counterName="varCurrentCustomer" name="forEachCustomer">
              <startCounterValue>1</startCounterValue>
              <finalCounterValue>count($varGetCustomersRs.GetCustomersRs/ns1:Return/ns1:Customer)</finalCounterValue>
              <scope name="scopeForEachCust">
                <variables>
                  <variable name="varCurrentCustomerData" element="ns1:GetCustomersRs"/>
                  <variable name="varFindPersonRq" messageType="ns4:FindPersonRq"/>
                  <variable name="varFindPersonRs" messageType="ns4:FindPersonRs"/>
                  <variable name="varFindOrganizationRq" messageType="ns4:FindOrganizationRq"/>
                  <variable name="varFindOrganizationRs" messageType="ns4:FindOrganizationRs"/>
                  <variable name="varUpdateCustomerRq" messageType="ns3:UpdateCustomerRq"/>
                  <variable name="varUpdateCustomerRs" messageType="ns3:UpdateCustomerRs"/>
                  <variable name="varCreateCustomerRq" messageType="ns2:CreateCustomerRq"/>
                  <variable name="varCreateCustomerRs" messageType="ns2:CreateCustomerRs"/>
                  <variable name="varOperationResult" type="ns0:OperationRsType"/>
                  <variable name="varUpdateCustomersRq" messageType="ns1:UpdateCustomersRq"/>
                  <variable name="varUpdateCustomersRs" messageType="ns1:UpdateCustomersRs"/>
                  <variable name="varFindErrorRs" type="cmn:ErrorsType"/>
                </variables>
                <sequence name="sequenceForEachCust">
                  <assign name="assignCurrentCustomer">
                    <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                      <from>$varGetCustomersRs.GetCustomersRs/ns1:Return/ns1:Customer[$varCurrentCustomer]</from>
                      <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCurrentCustomerData/ns1:Return/ns1:Customer</to>
                    </copy>
                  </assign>
                  <if name="IfCustomerIsPerson">
                    <documentation>
                      <![CDATA[Person]]>
                    </documentation>
                    <condition>$varCurrentCustomerData/ns1:Return/ns1:Customer[1]/ns1:CustomerType = 'PERSON'</condition>
                    <sequence name="sequenceFindPerson">
                      <assign name="trFindCustomerRq">
                        <bpelx:annotation>
                          <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                        </bpelx:annotation>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>ora:doXSLTransformForDoc("../Transformations/XSLs/trFindPersonRq.xsl", $varCurrentCustomerData)</from>
                          <to variable="varFindPersonRq" part="FindPersonRqPart"/>
                        </copy>
                      </assign>
                      <invoke name="invokeWsERPIntegrationServiceTecFindPerson" bpelx:invokeAsDetail="no"
                              partnerLink="wsERPIntegrationServiceTec" portType="ns4:ERPIntegrationTecPortType"
                              operation="FindPerson" inputVariable="varFindPersonRq" outputVariable="varFindPersonRs"/>
                      <assign name="assignRs">
                        <extensionAssignOperation>
                          <bpelx:copyList ignoreMissingFromData="yes">
                            <?audit suppress oracle.ide.xml.validation-incomplete?>
                            <bpelx:from expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFindPersonRs.FindPersonRsPart/cmn:Errors/cmn:Error</bpelx:from>
                            <bpelx:to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFindErrorRs/cmn:Error</bpelx:to>
                          </bpelx:copyList>
                        </extensionAssignOperation>
                      </assign>
                    </sequence>
                    <else>
                      <documentation>
                        <![CDATA[Organization]]>
                      </documentation>
                      <sequence name="sequenceFindOrganization">
                        <assign name="trFindCustomerRq">
                          <bpelx:annotation>
                            <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                          </bpelx:annotation>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>ora:doXSLTransformForDoc("../Transformations/XSLs/trFindOrganizationRq.xsl", $varCurrentCustomerData)</from>
                            <to variable="varFindOrganizationRq" part="FindOrganizationRqPart"/>
                          </copy>
                        </assign>
                        <invoke name="invokeWsERPIntegrationServiceTecFindOrganization" bpelx:invokeAsDetail="no"
                                partnerLink="wsERPIntegrationServiceTec" portType="ns4:ERPIntegrationTecPortType"
                                operation="FindOrganization" inputVariable="varFindOrganizationRq"
                                outputVariable="varFindOrganizationRs"/>
                        <assign name="assignRs">
                          <extensionAssignOperation>
                            <bpelx:copyList ignoreMissingFromData="yes">
                              <?audit suppress oracle.ide.xml.validation-incomplete?>
                              <bpelx:from expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFindOrganizationRs.FindOrganizationRsPart/cmn:Errors/cmn:Error</bpelx:from>
                              <bpelx:to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFindErrorRs/cmn:Error</bpelx:to>
                            </bpelx:copyList>
                          </extensionAssignOperation>
                        </assign>
                      </sequence>
                    </else>
                  </if>
                  <if name="IfCustomerExist">
                    <documentation>
                      <![CDATA[Update Customer]]>
                    </documentation>
                    <condition>(count($varFindOrganizationRs.FindOrganizationRsPart/ns4:Customer[1]/ns4:Organization) &gt;0 or count($varFindPersonRs.FindPersonRsPart/ns4:Customer[1]/ns4:Person) &gt;0 ) and ( count($varFindErrorRs/cmn:Error) = 0 )</condition>
                    <sequence name="Sequence1">
                      <assign name="trUpdateCustomerRq">
                        <bpelx:annotation>
                          <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                        </bpelx:annotation>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>ora:doXSLTransformForDoc("../Transformations/XSLs/trUpdateCustomerRq.xsl", $varCurrentCustomerData, "varFindPersonRs.FindPersonRsPart", $varFindPersonRs.FindPersonRsPart, "varFindOrganizationRs.FindOrganizationRsPart", $varFindOrganizationRs.FindOrganizationRsPart)</from>
