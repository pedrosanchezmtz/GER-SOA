                        </copy>
                        <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                          <from>$inputVariable.UpdateUuidRq/ns0:ListUpdateUuidRq[$counterUui]/ns0:Object/ns4:UserKeyD</from>
