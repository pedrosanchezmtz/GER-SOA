         xmlns:ns2="http://platform.integration.oracle/blocks/adapter/fw/metadata/ReceiveEmail"
         xmlns:ns3="http://soa.estrellaroja.com.mx/ERPIntegrationBiz"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns4="http://soa.estrellaroja.com.mx/EstrellaRojaCommons"
         xmlns:ns5="http://soa.estrellaroja.com.mx/Reports/XXER_ValidationComplement"
         xmlns:ns6="http://soa.estrellaroja.com.mx/ValidationPaymentsAPBiz/BpelSendNotifications"
         xmlns:ns7="http://soa.estrellaroja.com.mx/ValidationPaymentsAPBiz"
         xmlns:ns8="http://www.sat.gob.mx/cfd/3"
         xmlns:ns9="http://www.sat.gob.mx/TimbreFiscalDigital"
         xmlns:ns10="http://soa.estrellaroja.com.mx/SOAUtilitiesTec"
         xmlns:ns11="http://soa.estrellaroja.com.mx/ERPIntegrationServiceTec"
         xmlns:ns12="http://soa.estrellaroja.com.mx/AuditControlTec" xmlns:ns13="http://www.sat.gob.mx/Pagos"
         xmlns:ns14="http://xmlns.oracle.com/pcbpel/adapter/opaque/"
         xmlns:sp1="http://soa.estrellaroja.com.mx/ValidationPaymentsAPBiz/SBpelValidateLines"
         xmlns:ns15="http://www.sat.gob.mx/cfd/4" xmlns:ns18="http://www.sat.gob.mx/ConsumoDeCombustibles11"
         xmlns:ns19="http://www.sat.gob.mx/EstadoDeCuentaCombustible12" xmlns:ns17="http://www.sat.gob.mx/CartaPorte20"
         xmlns:ns20="http://www.sat.gob.mx/nomina12" xmlns:ns16="http://www.sat.gob.mx/Pagos20"
         xmlns:ns21="http://soa.estrellaroja.com.mx/CFDITec" xmlns:ns23="http://soa.estrellaroja.com.mx/CFDI">
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <import namespace="http://www.sat.gob.mx/cfd/4" location="oramds:/apps/Common/XSD/CFDI40/cfdv40.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://www.sat.gob.mx/cfd/3" location="oramds:/apps/Common/XSD/CFDI/cfdv33.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://soa.estrellaroja.com.mx/Reports/XXER_ValidationComplement"
          location="oramds:/apps/Common/Report/XXER_ValidationComplement.xsd"
          importType="http://www.w3.org/2001/XMLSchema"/>
  <import namespace="http://xmlns.oracle.com/pcbpel/adapter/ums/GER_SOA_DEV_OCI/ValidationPaymentsAPBiz/ReceiveEmail"
          location="../WSDLs/External/ReceiveEmail.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"
          ui:processWSDL="true"/>
    <import location="oracle.xml.parser.v2.XMLElement" importType="http://schemas.oracle.com/bpel/extension/java"/>

  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="ReceiveEmail" partnerLinkType="ns1:ReceiveAttachments_plt" myRole="ReceiveAttachments_role"/>
    <partnerLink name="WsERPIntegrationBiz" partnerLinkType="ns3:WsERPIntegrationBiz"
                 partnerRole="ERPIntegrationBizPortType"/>
    <partnerLink name="BpelSendNotifications" partnerLinkType="ns6:BpelSendNotifications"
                 partnerRole="BpelSendNotificationsProvider"/>
    <partnerLink name="WsSOAUtilitiesTec" partnerLinkType="ns10:WsSOAUtilitiesTec"
                 partnerRole="SOAUtilitiesTecPortType"/>
    <partnerLink name="WsERPIntegrationServiceTec" partnerLinkType="ns11:WsERPIntegrationServiceTec"
                 partnerRole="ERPIntegrationTecPortType"/>
    <partnerLink name="wsAuditControlTec" partnerLinkType="ns12:wsAuditControlTec"
                 partnerRole="AuditControlTecPortType"/>
    <partnerLink name="wsCFDITec" partnerLinkType="ns21:wsCFDITec" partnerRole="CFDITecPortType"
                 myRole="CFDITecPortTypeCallback"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="ns1:ReceiveAttachments_msg"/>
    <variable name="v_payment_id" type="xsd:string"/>
    <variable name="v_payment" element="ns5:DATA_DS"/>
    <variable name="WsNotificationEmailRq" messageType="ns6:BpelSendNotificationsRequestMessage"/>
    <variable name="v_paymentXML" element="ns8:Comprobante"/>
    <variable name="wsAuditControlTecMergeComplementsAPRq" messageType="ns12:MergeComplementsAPRq"/>
    <variable name="wsAuditControlTecMergeComplementsAPRs" messageType="ns12:MergeComplementsAPRs"/>
    <variable name="v_paymentXML40" element="ns15:Comprobante"/>
    <variable name="GetComplementCFDIRq" messageType="ns21:GetComplementCFDIRqMsg"/>
    <variable name="GetComplementCFDIRs" messageType="ns21:GetComplementCFDIRsMsg"/>
  </variables>
  <faultHandlers>
    <catchAll>
      <empty name="Empty1"/>
    </catchAll>
  </faultHandlers>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in BpelValidateAttachmentsAP.wsdl) --><receive name="receiveInput"
                                                                                                                            partnerLink="ReceiveEmail"
                                                                                                                            portType="ns1:ReceiveAttachments_ptt"
                                                                                                                            operation="ReceiveAttachments" variable="inputVariable" createInstance="yes"/>
    <scope name="ScopePaymentId">
      <variables>
        <variable name="v_bodyString" type="xsd:string"/>
      </variables>
      <sequence name="SeqPaymentId">
        <extensionActivity>
          <bpelx:exec name="Java_Embedding1">
            <![CDATA[addAuditTrailEntry("LimpiaCadena");                                                  
try {                                               
XMLElement input = (XMLElement) getVariableData("inputVariable","body","ns14:opaqueElement");                                                  
String input_str = input.getTextContent();                                                  
addAuditTrailEntry("Base64decode input_str="+input_str);                                                  
oracle.soa.common.util.Base64Decoder decoder = new oracle.soa.common.util.Base64Decoder();                                                      
String decoded = null;                                                     
decoded = decoder.decode(input_str);                                                                                                    
addAuditTrailEntry("Base64decode decoded="+decoded);     
String cadena = null;  
String cadena2 = null;  
cadena2= decoded.replaceAll("\\<.*?>","");  
addAuditTrailEntry("sin html="+cadena2);   
cadena=cadena2.replace("\n", "");    
decoded=cadena.replace("\r", "");     
addAuditTrailEntry("Cadena sin saltos="+decoded);     
cadena=decoded.replace(">","");     
addAuditTrailEntry("Cadena sin mas menos ="+cadena);     
decoded=cadena.replace(" ","");     
addAuditTrailEntry("Cadena sin espacios="+decoded);  
cadena=decoded.replace("*","");     
addAuditTrailEntry("Cadena sin asteriscos="+cadena);  
setVariableData("v_bodyString",cadena);                                                  
} catch (Exception e) {                                                  
  addAuditTrailEntry("Base64decode Exception: "+e.getMessage());                                                  
}                                                  
addAuditTrailEntry("Base64decode ended");]]>
          </bpelx:exec>
        </extensionActivity>
        <assign name="AssignPaymentId">
          <copy ignoreMissingFromData="yes">
            <from>substring-before(substring-after($v_bodyString,"NUMEROIDPAGOORACLE:"),"NUMERODEPAGO:")</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_payment_id</to>
          </copy>
        </assign></sequence></scope>
    <if name="If_ExistPaymentID">
      <documentation>
        <![CDATA[SinPaymentId]]>
      </documentation>
      <condition>string-length($v_payment_id)=0</condition>
      <exit name="Exit1"/>
    </if>
    <scope name="ScopeGetPaymentAPOLO">
      <variables>
        <variable name="WsErpIntegrationBizGenerateReportRq" messageType="ns3:GenerateReportRq"/>
        <variable name="WsErpIntegrationBizGenerateReportRs" messageType="ns3:GenerateReportRs"/>
        <variable name="v_stringXML" type="xsd:string"/>
      </variables>
      <sequence name="SeqGetPaymentAPOLO"><assign name="assignParams"
                                                                                                                   xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
            <from>dvm:lookupValue("DVMs/dvmValidationPaymentsBizConfigs.dvm","Code","TargetInterface","Value","","Component","ValidationPaymentsBiz")</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:TargetInterface</to>
          </copy>
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
            <from>ora:getCompositeName()</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:IntegrationName</to>
          </copy>
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
            <from>ora:getComponentName()</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:ProcessName</to>
          </copy>
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
            <from>dvm:lookupValue("DVMs/dvmValidationPaymentsBizConfigs.dvm","Code","ReportName","Value","","Component","GetPayment")</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:ReportName</to>
          </copy>
          <copy bpelx:insertMissingToData="yes">
            <from>'p_numPago'</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[1]/ns3:name</to>
          </copy>
          <copy bpelx:insertMissingToData="yes">
            <from>$v_payment_id</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsErpIntegrationBizGenerateReportRq.GenerateReportRq/ns3:parameterNameValues/ns3:item[1]/ns3:values/ns3:item[1]</to>
          </copy>
        </assign><invoke name="WsErpIntegrationBiz" partnerLink="WsERPIntegrationBiz"
                    portType="ns3:ERPIntegrationBizPortType" operation="GenerateReport"
                    inputVariable="WsErpIntegrationBizGenerateReportRq"
                    outputVariable="WsErpIntegrationBizGenerateReportRs"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable" bpelx:invokeAsDetail="no"/>
        <assign name="AssignParseStringXML" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
          <copy ignoreMissingFromData="yes">
            <from>concat('&lt;DATA_DS xmlns="http://soa.estrellaroja.com.mx/Reports/XXER_ValidationComplement"&gt;',substring-after($WsErpIntegrationBizGenerateReportRs.GenerateReportRs/ns3:Return/ns3:xmlOut,'&lt;DATA_DS&gt;'))</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_stringXML</to>
          </copy>
          <copy ignoreMissingFromData="yes">
            <from>oraext:parseXML($v_stringXML)</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_payment</to>
          </copy>
        </assign>
      </sequence></scope>
    <if name="If_Exist_Pago">
      <documentation>
        <![CDATA[Pago]]>
      </documentation>
      <condition>count($v_payment/ns5:G_1)&gt;0</condition>
      <sequence>
        <sequence name="SequencePagoApolo">
          <assign name="Assign1">
            <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$GetComplementCFDIRq.GetComplementCFDIRqPart/ns5:DATA_DS/ns5:G_1</to>
            </copy>
            <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
              <from>true()</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$GetComplementCFDIRq.GetComplementCFDIRqPart/ns21:GetDataResponse</to>
            </copy>
            <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
              <from>true()</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$GetComplementCFDIRq.GetComplementCFDIRqPart/ns21:SendReport</to>
            </copy>
          </assign>
          <invoke name="GetComplementCFDI" partnerLink="wsCFDITec"
                                                   portType="ns21:CFDITecPortType" operation="GetComplementCFDI"
                                                   inputVariable="GetComplementCFDIRq"
                                                   outputVariable="GetComplementCFDIRs" bpelx:invokeAsDetail="no"
                                                   xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
                                                   xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/><forEach parallel="no" counterName="invoiceLinesCounter" name="For_Lines">
            <startCounterValue>1</startCounterValue>
            <finalCounterValue>count($v_payment/ns5:G_1[1]/ns5:G_2)</finalCounterValue>
            <scope name="ScopeLines">
              <assign name="AssignLineFactura" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                <copy>
                  <from>concat($WsNotificationEmailRq.payload/ns7:DETAIL_LINES, "&lt;tr&gt;&lt;td&gt;",$v_payment/ns5:G_1/ns5:G_2[$invoiceLinesCounter]/ns5:NUMERO_DOCUMENTO,"&lt;/td&gt;&lt;td&gt;",$GetComplementCFDIRs.GetComplementCFDIRsPart/ns21:COMPLEMENT[1]/ns21:COMPLEMENT_LINE[ns12:GetInvoicesAPByExampleRs/ns12:Return/ns12:Invoice[1]/ns12:invoiceId=$v_payment/ns5:G_1/ns5:G_2[$invoiceLinesCounter]/ns5:ID_FACTURA]/ns12:GetInvoicesAPByExampleRs/ns12:Return/ns12:Invoice[1]/ns12:uuid,"&lt;/td&gt;&lt;td&gt;",$v_payment/ns5:G_1/ns5:G_2[$invoiceLinesCounter]/ns5:MONTO_DOCUMENTO,"&lt;/td&gt;&lt;/tr&gt;")</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:DETAIL_LINES</to>
                </copy>
              </assign>
            </scope>
          </forEach>
          <assign name="AssignPagoHeader">
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:ID_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:paymentId</to>
            </copy>
            <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:REGIMEN_EMPRESA</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:receptorRegimen</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:NUMERO_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:paymentNumber</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:RFC_PROVEEDOR</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:emisorRfc</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:NOMBRE_PROVEEDOR</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:nombreEmisor</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:RFC_EMPRESA</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:receptorRfc</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:NOMBRE_EMPRESA</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:nombrePreceptor</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:MONTO_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:paymentAmount</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:MONEDA_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:paymentCurrency</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>xp20:format-dateTime($v_payment/ns5:G_1[1]/ns5:FECHA_PAGO,"[Y0001]-[M01]-[D01]")</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:paymentDate</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>ora:getCompositeName()</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$wsAuditControlTecMergeComplementsAPRq.MergeComplementsAPRq/ns12:Complements/ns12:Complement/ns12:createdBy</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:ID_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:ID_PAGO</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:ID_CHECK</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:ID_CHECK</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:MONTO_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:MONTO_PAGO</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>xp20:format-dateTime($v_payment/ns5:G_1[1]/ns5:FECHA_PAGO,"[Y0001]-[M01]-[D01]")</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:FECHA_PAGO</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:EMAIL_NOTIFICACION</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:EMAIL_NOTIFICACION</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:REFERENCE_NUMBER</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:REFERENCE_NUMBER</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:NUMERO_PAGO</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:NUMERO_PAGO</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:NOMBRE_PROVEEDOR</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:NOMBRE_PROVEEDOR</to>
            </copy>
            <copy ignoreMissingFromData="yes">
              <from>$v_payment/ns5:G_1[1]/ns5:MENSAJE</from>
              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$WsNotificationEmailRq.payload/ns7:NUMERO_NOTIFICACION</to>
