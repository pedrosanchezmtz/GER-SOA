         xmlns:ns1="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/StatementsBankHSBC">
  <import namespace="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/StructureBankDetails"
           location="../Schemas/StructureBankDetails.xsd" importType="http://www.w3.org/2001/XMLSchema"/>
   <import namespace="http://soa.estrellaroja.com.mx/StructureCodeBank" location="../Schemas/StructureBankCode.xsd"
           importType="http://www.w3.org/2001/XMLSchema"/>
   <import namespace="http://soa.estrellaroja.com.mx/BankCodes" location="../Schemas/BankCodes.xsd"
           importType="http://www.w3.org/2001/XMLSchema"/>

  <import ui:processWSDL="true" namespace="http://soa.estrellaroja.mx/SFTPEstrellaRojaEnt/bpelManagementsStatementsBankHSBC" location="../WSDLs/bpelManagementsStatementsBankHSBC.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="bpelmanagementsstatementsbankhsbc_client" partnerLinkType="client:bpelManagementsStatementsBankHSBC" myRole="bpelManagementsStatementsBankHSBCProvider"/>
      <partnerLink name="wsSFTPEstrellaRojaBiz" partnerLinkType="ns2:wsSFTPEstrellaRojaBiz"
                   partnerRole="SFTPEstrellaRojaBizPortType"/>
   </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="client:bpelManagementsStatementsBankHSBCRequestMessage"/>
    <variable name="varInstanceTitle" type="xsd:string"/>
    <variable name="varFileName" type="xsd:string"/>
    <variable name="v_saldoInicial" type="xsd:string"/>
    <variable name="v_saldoFinal" type="xsd:string"/>
    <variable name="v_credito" type="xsd:string"/>
    <variable name="v_debito" type="xsd:string"/>
    <variable name="v_inverso" type="xsd:boolean">
      <from>false()</from>
    </variable>
  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in bpelManagementsStatementsBankHSBC.wsdl) --><receive name="receiveInput" partnerLink="bpelmanagementsstatementsbankhsbc_client" portType="client:bpelManagementsStatementsBankHSBC" operation="process" variable="inputVariable" createInstance="yes">
      <bpelx:fromProperties>
        <bpelx:fromProperty name="jca.ftp.FileName" variable="varFileName"/>
      </bpelx:fromProperties>
    </receive>
    <scope name="scopeMain" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <documentation>
         <![CDATA[Scope principal que contiene las asignaciones, invocaciones a servicioes de negocio, así mismo transformaciones para el gestionamiento de lo esados de cuentas del banco de banorte.]]>
      </documentation>
      <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
         <bpelx:general>
            <bpelx:property name="userDescription"><![CDATA[
               ]]><![CDATA[Scope principal que contiene las asignaciones, invocaciones a servicioes de negocio, así mismo transformaciones para el gestionamiento de lo esados de cuentas del banco de banorte.]]>
            </bpelx:property>
         </bpelx:general>
      </bpelx:annotation>
      <variables>
         <variable name="varInvokeWsSFTPEstrellaRojaGetBankStatementsRq" messageType="ns2:GetBankStatementsRq"/>
         <variable name="varInvokeWsSFTPEstrellaRojaGetBankStatementsRs" messageType="ns2:GetBankStatementsRs"/>
         <variable name="varCodes" element="ns4:CodigoList"/>
         <variable name="varCodesList" type="xsd:string"/>
         <variable name="varDataCode" element="ns5:DATA_DS"/>
         <variable name="varDataCodeReport" element="ns5:DATA_DS"/>
         <variable name="varFlagError" type="xsd:boolean"/>
         <variable name="varStringError" type="xsd:string"/>
         <variable name="varMessageRq" messageType="ns8:MessageRq"/>
         <variable name="varMessageRs" type="ns6:BaseRsType"/>
      </variables>
      <sequence name="Sequence1">
         <assign name="assignInitialize">
            
            
         <copy bpelx:insertMissingToData="yes" xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
               <from>false()</from>
               <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFlagError</to>
            </copy><copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                         xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
               <from>oraext:setFlowInstanceTitle("ImportHSBCBank")</from>
               <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInstanceTitle</to>
            </copy></assign>
         <scope name="scopeGenerateReport">
            <variables>
               <variable name="varInvokeWSGenerateReportRq" messageType="ns10:GenerateReportRq"/>
               <variable name="varInvokeWSGenerateReportRs" messageType="ns10:GenerateReportRs"/>
               <variable name="varGetBankDetailsRq" element="ns7:GetBankDetailsRq"/>
               <variable name="varGetBankDetailsRs" element="ns7:GetBankDetailsRs"/>
               <variable name="varCountStatements" type="xsd:int">
                  <from>
                     <literal>0</literal>
                  </from>
               </variable>
            </variables>
            <faultHandlers>
               <catchAll>
                  <assign name="assignCatchAll" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>true()</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFlagError</to>
                </copy>
                <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes"
                      xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                  <from>ora:getFaultAsString()</from>
                  <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varStringError</to>
                </copy>
              </assign>
               </catchAll>
            </faultHandlers>
            <sequence name="Sequence2">
               <assign name="trAccountStatementsTobpelRq.xsl">
                  <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                     <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                  </bpelx:annotation>
                  <copy>
                     <from>ora:doXSLTransformForDoc("../Transformations/trAccountStatementsHSBC_To_bpelAccountStatementsHSBCRq.xsl", $inputVariable.payload)</from>
                     <to variable="inputVariable" part="payload"/>
                  </copy>
               </assign>
            <assign name="Assign1">
              <copy>
                <from>sum($inputVariable.payload/ns1:StatementsLine/ns1:ImporteCredito)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_credito</to>
              </copy>
              <copy>
                <from>sum($inputVariable.payload/ns1:StatementsLine/ns1:ImporteDebito)</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$v_debito</to>
              </copy>
            </assign>
            <extensionActivity>
              <bpelx:exec name="Java_Embedding1">
                <![CDATA[addAuditTrailEntry("LimpiaCadena");                                                   
try {                                                
String v_credito = (String)getVariableData("v_credito");                                                   
addAuditTrailEntry("total de credito="+ v_credito); 
String v_debito = (String)getVariableData("v_debito");                                                   
addAuditTrailEntry("total de debito="+ v_debito);     
                                               
} catch (Exception e) {                                                   
  addAuditTrailEntry("Base64decode Exception: "+e.getMessage());                                                   
}]]>
              </bpelx:exec>
            </extensionActivity>
            <assign name="trGetBankCodeSubtrsToBpel"
                       xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                  <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                     <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                  </bpelx:annotation>
                  <copy>
                     <from>ora:doXSLTransformForDoc("../Transformations/trGetBankCodeSubtrs_To_BpelHSBC.xsl", $inputVariable.payload)</from>
                     <to variable="varCodes"/>
                  </copy>
               </assign>
               <assign name="assignGenerateReportRq" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>oraext:create-delimited-string($varCodes/ns4:Codigo,',')</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCodesList</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>$varCodesList</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:parameterNameValues/ns10:item[1]/ns10:values/ns10:item[1]</to>
              </copy>
              <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'PTRXCODE', 'Value', '' )</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:parameterNameValues/ns10:item[1]/ns10:name</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'TargetInterface', 'Value',' ')</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:TargetInterface</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'IntegrationName', 'Value','')</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:IntegrationName</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'ProcessName', 'Value' ,'')</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:ProcessName</to>
              </copy>
              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'ReportName', 'Value' ,'')</from>
                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns10:ReportName</to>
              </copy>
            </assign>
               <extensionActivity>
                  <bpelx:call name="CallSubProcessGenerateReport"
                              xmlns:sp1="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/sbpelGenerateReport"
                              target="sp1:sbpelGenerateReport" xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                     <bpelx:param name="varInvokeWSGenerateReportRq" copyByValue="yes"
                                  variable="varInvokeWSGenerateReportRq"/>
                     <bpelx:param name="varInvokeWSGenerateReportRs" copyByValue="no"
                                  variable="varInvokeWSGenerateReportRs"/>
                  </bpelx:call>
               </extensionActivity>
            <if name="IfValidateReportSuccess">
                  <documentation>
                     <![CDATA[Success]]>
                  </documentation>
                  <condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">count($varInvokeWSGenerateReportRs.GenerateReportRs/ns6:Success)&gt;0</condition>
                  <sequence>
                     <assign name="assignWsERPIntegrationBizGenerateReportRs"
                             xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                        <copy>
                           <from>oraext:parseXML($varInvokeWSGenerateReportRs.GenerateReportRs/ns10:Return/ns10:xmlOut)</from>
                           <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varDataCode</to>
                        </copy>
                     </assign>
                <assign name="trVarDataCodeToVarDataCodeReport"
                             xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                        <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                           <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                        </bpelx:annotation>
                        <copy>
                           <from>ora:doXSLTransformForDoc("../Transformations/trVarDataCode_To_VarDataCodeReport.xsl", $varDataCode)</from>
                           <to variable="varDataCodeReport"/>
                        </copy>
                     </assign>
                <if name="IfValidateData">
                        <documentation>
                           <![CDATA[Found Codes in ERP]]>
                        </documentation>
                        <condition>count($varDataCodeReport/ns5:XXER_CE_CODIGO_BANCARIO)&gt;0</condition>
                        <sequence>
                           <assign name="trbpelToSFTPEstrellaRojaBizGetBankStatementsRq">
                              <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                 <bpelx:pattern patternName="bpelx:xquery"></bpelx:pattern>
                              </bpelx:annotation>
                              <copy>
                                 <from>ora:processXQuery10('../Transformations/trbpel_To_SFTPEstrellaRojaBizGetBankStatementsHSBCRq.xqy', 'varDataCodeReport', $varDataCodeReport, 'inputVariable.payload', $inputVariable.payload)</from>
                                 <to variable="varInvokeWsSFTPEstrellaRojaGetBankStatementsRq"
                                     part="GetBankStatementsRq"/>
                              </copy>
                           </assign>
                    <assign name="assingAccountNumber">
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                 <from>$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists[1]/ns2:AccountNumber</from>
                                 <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetBankDetailsRq/ns7:AccountNumber</to>
                              </copy>
                           </assign>
                           <extensionActivity>
                              <bpelx:call name="CallSubProcessGetBankDetails"
                                          xmlns:sp1="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/sbpelGetBankDetails"
                                          target="sp1:sbpelGetBankDetails"
                                          xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                 <bpelx:param name="varGetBankDetailsRq" copyByValue="yes"
                                              variable="varGetBankDetailsRq"/>
                                 <bpelx:param name="varGetBankDetailsRs" copyByValue="no"
                                              variable="varGetBankDetailsRs"/>
                              </bpelx:call>
                           </extensionActivity>
                           <if name="ifValidateGetBankDetails">
                              <documentation>
                                 <![CDATA[Success and Fund Data]]>
                              </documentation>
                              <condition>$varGetBankDetailsRs/ns6:Success and ( string-length($varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BankName)&gt;0 and string-length($varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BranchName)&gt;0 )</condition>
                              <sequence>
                        <assign name="assingBankDetails">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BankName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankName</to>
                          </copy>
                          <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from><literal>HSBC</literal></from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Sufix</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BranchName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankBranch</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>ora:getComponentInstanceId()</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BanchId</to>
                          </copy>
                        </assign>
                        <if name="If_Orden">
                          <condition>(((number($inputVariable.payload/ns1:StatementsLine[1]/ns1:Saldo) - number($inputVariable.payload/ns1:StatementsLine[1]/ns1:ImporteCredito))+number($v_credito))-number($v_debito))=number($inputVariable.payload/ns1:StatementsLine[last()]/ns1:Saldo)</condition>
                          <sequence>
                            <assign name="assignBalanceStart"
                                    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                <from>count($varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists)</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCountStatements</to>
                              </copy>
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                <from>$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists[1]/ns2:AccountNumber</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:AccountNumber</to>
                              </copy>
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                <from>"OPBD"</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:StatementCode</to>
                              </copy>
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:CurrencyCode</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:CurrencyCode</to>
                              </copy>
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                    xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                                <from>"CRFT"</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:Indicator</to>
                              </copy>
                              <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                                <from>number(translate($inputVariable.payload/ns1:StatementsLine[1]/ns1:Saldo,',',''))-number(translate($inputVariable.payload/ns1:StatementsLine[1]/ns1:ImporteCredito,',',''))+number(translate($inputVariable.payload/ns1:StatementsLine[1]/ns1:ImporteDebito,',',''))</from>
                                <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:Balance</to>
