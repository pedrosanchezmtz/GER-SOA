   <import ui:processWSDL="true" namespace="http://xmlns.oracle.com/SFTPEstrellaRojaEnt/SFTPEstrellaRojaEnt/bpelManagementsStatmentsBankBBVA" location="../WSDLs/bpelManagementsStatmentsBankBBVA.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
 <import ui:processWSDL="true" namespace="http://soa.estrellaroja.mx/SFTPEstrellaRojaEnt/bpelManagementsStatementsBankBanorte" location="../WSDLs/bpelManagementsStatementsBankBanorte.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
