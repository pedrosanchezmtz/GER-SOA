            <variable name="varCodesList" type="xsd:string"/>
            <variable name="varDataCode" element="ns5:DATA_DS"/>
            <variable name="varDataCodeReport" element="ns5:DATA_DS"/>
        <variable name="varFlagError" type="xsd:boolean"/>
        <variable name="varStringError" type="xsd:string"/>
        <variable name="varMessageRq" messageType="ns8:MessageRq"/>
        <variable name="varMessageRs" type="ns6:BaseRsType"/>
      </variables>
         <sequence name="Sequence1">
        <assign name="assignInitialize">
          <copy bpelx:insertMissingToData="yes">
            <from>false()</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFlagError</to>
          </copy>
          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
            <from>oraext:setFlowInstanceTitle("ImportBancomerBank")</from>
            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInstanceTitle</to>
          </copy>
        </assign>
        <scope name="scopeGenerateReport">
            <variables>
               <variable name="varInvokeWSGenerateReportRq" messageType="ns1:GenerateReportRq"/>
               <variable name="varInvokeWSGenerateReportRs" messageType="ns1:GenerateReportRs"/>
            <variable name="varGetBankDetailsRq" element="ns7:GetBankDetailsRq"/>
            <variable name="varGetBankDetailsRs" element="ns7:GetBankDetailsRs"/>
            <variable name="varCountStatements" type="xsd:int">
              <from><literal>0</literal></from>
            </variable>
          </variables>
          <faultHandlers>
            <catchAll><assign name="assignCatchAll" xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
            xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
         <from>true()</from>
         <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varFlagError</to>
      </copy>
      <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes"
            xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
         <from>ora:getFaultAsString()</from>
         <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varStringError</to>
      </copy>
   </assign></catchAll>
          </faultHandlers>
          <sequence name="Sequence2">
            <assign name="trAccountStatementsTobpelRq.xsl">
              <bpelx:annotation>
                <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
              </bpelx:annotation>
              <copy>
                <from>ora:doXSLTransformForDoc("../Transformations/trAccountStatements_To_bpelAccountStatementsRq.xsl", $inputVariable.payload)</from>
                <to variable="inputVariable" part="payload"/>
              </copy>
            </assign>
            <assign name="trGetBankCodeSubtrsToBpel"
                                                  xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
      <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
         <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
      </bpelx:annotation>
      <copy>
         <from>ora:doXSLTransformForDoc("../Transformations/trGetBankCodeSubtrs_To_BpelBBVA.xsl", $inputVariable.payload)</from>
         <to variable="varCodes"/>
      </copy>
   </assign><assign name="assignGenerateReportRq"
                                                  xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                        <from>oraext:create-delimited-string($varCodes/ns4:Codigo,',')</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCodesList</to>
                     </copy>
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>$varCodesList</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:parameterNameValues/ns1:item[1]/ns1:values/ns1:item[1]</to>
                     </copy>
                     <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'PTRXCODE', 'Value', '' )</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:parameterNameValues/ns1:item[1]/ns1:name</to>
                     </copy>
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'TargetInterface', 'Value',' ')</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:TargetInterface</to>
                     </copy>
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'IntegrationName', 'Value','')</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:IntegrationName</to>
                     </copy>
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'ProcessName', 'Value' ,'')</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:ProcessName</to>
                     </copy>
                     <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                           xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                        <from>dvm:lookupValue ('DVMs/dvmGetTrxType.dvm', 'Code', 'ReportName', 'Value' ,'')</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWSGenerateReportRq.GenerateReportRq/ns1:ReportName</to>
                     </copy>
                  </assign>
                  <extensionActivity>
                     <bpelx:call name="CallSubProcessGenerateReport"
                                 xmlns:sp1="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/sbpelGenerateReport"
                                 target="sp1:sbpelGenerateReport"
                                 xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                <bpelx:param name="varInvokeWSGenerateReportRq" copyByValue="yes"
                             variable="varInvokeWSGenerateReportRq"/>
                <bpelx:param name="varInvokeWSGenerateReportRs" copyByValue="no"
                             variable="varInvokeWSGenerateReportRs"/>
              </bpelx:call>
                  </extensionActivity>
                  <if name="IfValidateReportSuccess">
                     <documentation>
                        <![CDATA[Success]]>
                     </documentation>
                     <condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">count($varInvokeWSGenerateReportRs.GenerateReportRs/ns6:Success)&gt;0</condition>
                     <sequence>
                        <assign name="assignWsERPIntegrationBizGenerateReportRs"
                                xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                           <copy>
                              <from>oraext:parseXML($varInvokeWSGenerateReportRs.GenerateReportRs/ns1:Return/ns1:xmlOut)</from>
                              <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varDataCode</to>
                           </copy>
                        </assign>
                        <assign name="trVarDataCodeToVarDataCodeReport"
                                xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                           <bpelx:annotation xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                              <bpelx:pattern patternName="bpelx:transformation"></bpelx:pattern>
                           </bpelx:annotation>
                           <copy>
                              <from>ora:doXSLTransformForDoc("../Transformations/trVarDataCode_To_VarDataCodeReport.xsl", $varDataCode)</from>
                              <to variable="varDataCodeReport"/>
                           </copy>
                        </assign>
                        <if name="IfValidateData">
                           <documentation>
                              <![CDATA[Found Codes in ERP]]>
                           </documentation>
                           <condition>count($varDataCodeReport/ns5:XXER_CE_CODIGO_BANCARIO)&gt;0</condition>
                           <sequence>
                              <assign name="trbpelToSFTPEstrellaRojaBizGetBankStatementsRq">
                                 <bpelx:annotation>
                                    <bpelx:pattern patternName="bpelx:xquery"></bpelx:pattern>
                                 </bpelx:annotation>
                                 <copy>
                                    <from>ora:processXQuery10('../Transformations/trbpel_To_SFTPEstrellaRojaBizGetBankStatementsBBVARq.xqy', 'varDataCodeReport', $varDataCodeReport, 'inputVariable.payload', $inputVariable.payload)</from>
                                    <to variable="varInvokeWsSFTPEstrellaRojaGetBankStatementsRq"
                                        part="GetBankStatementsRq"/>
                                 </copy>
                              </assign>
                    <assign name="assingAccountNumber">
                      <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                        <from>$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists[1]/ns2:AccountNumber</from>
                        <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varGetBankDetailsRq/ns7:AccountNumber</to>
                      </copy>
                    </assign>
                    <extensionActivity>
                      <bpelx:call name="CallSubProcessGetBankDetails"
                                  xmlns:sp1="http://soa.estrellaroja.com.mx/SFTPEstrellaRojaEnt/sbpelGetBankDetails"
                                  target="sp1:sbpelGetBankDetails">
                        <bpelx:param name="varGetBankDetailsRq" copyByValue="yes" variable="varGetBankDetailsRq"/>
                        <bpelx:param name="varGetBankDetailsRs" copyByValue="no" variable="varGetBankDetailsRs"/>
                      </bpelx:call>
                    </extensionActivity>
                    <if name="ifValidateGetBankDetails">
                                 <documentation>
                                    <![CDATA[Success and Fund Data]]>
                                 </documentation>
                                 <condition>$varGetBankDetailsRs/ns6:Success and ( string-length($varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BankName)&gt;0 and string-length($varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BranchName)&gt;0  )</condition>
                                 <sequence>
                                    <assign name="assingBankDetails">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BankName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankName</to>
                          </copy>
                          <copy bpelx:insertMissingToData="yes" ignoreMissingFromData="yes">
                            <from><literal>BBVA</literal></from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Sufix</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:BranchName</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankBranch</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>ora:getComponentInstanceId()</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BanchId</to>
                          </copy>
                        </assign><assign name="assignBalanceStart"
                                         xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>count($varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists)</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varCountStatements</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists[1]/ns2:AccountNumber</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:AccountNumber</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>"CLBD"</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:StatementCode</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>number($inputVariable.payload/ns3:Statements[ns3:TipoDeMovimiento = "2" or ns3:TipoDeMovimiento = "3"]/ns3:Importe)-(sum($inputVariable.payload/ns3:Statements[ns3:TipoDeMovimiento = "0"]/ns3:Importe)+sum($inputVariable.payload/ns3:Statements[ns3:TipoDeMovimiento = "1"]/ns3:Importe))</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:Balance</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>$varGetBankDetailsRs/ns7:Return/ns7:BankDetails/ns7:CurrencyCode</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:CurrencyCode</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>"CRDT"</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:Indicator</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes"
                                xmlns:bpelx="http://schemas.oracle.com/bpel/extension">
                            <from>$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:BankLinesDetails/ns2:AccountStatementsLists[1]/ns2:StatementDate</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:BalanceDate</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>"10"</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[1]/ns2:OrigBalanceCode</to>
                          </copy>
                        </assign>
                        <assign name="assignBalanceFinish">
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>$inputVariable.payload/ns3:Statements[ns3:TipoDeMovimiento = "2" or ns3:TipoDeMovimiento = "3"]/ns3:Cuenta</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[2]/ns2:AccountNumber</to>
                          </copy>
                          <copy ignoreMissingFromData="yes" bpelx:insertMissingToData="yes">
                            <from>"OPBD"</from>
                            <to expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0">$varInvokeWsSFTPEstrellaRojaGetBankStatementsRq.GetBankStatementsRq/ns2:Balances/ns2:Balance[2]/ns2:StatementCode</to>
                          </copy>
